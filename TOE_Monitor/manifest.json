{
"name": "TOE_Monitor",
"version": "0.0.0.1",
"background":{
"scripts": ["scripts/background.js"],
"matches": ["<all_urls>"]
},

"manifest_version": 2,

"icons":{
"128": "img/icon.png"
},
"browser_action":{
"default_title": "TOE_Monitor",
"default_icon": "img/icon.png"

},

"content_scripts":[{
    "matches": ["*://*/*"],
    "js": ["scripts/content.js"]
  }],

"permissions": [

  "<all_urls>",
  "*://*/*",
	"unlimitedStorage",
  "tabs",
  "activeTab"


		    ]


}
